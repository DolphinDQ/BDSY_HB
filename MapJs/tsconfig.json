{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "preserveConstEnums": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "module": "es6",
    "target": "es5",
    "outDir": "./dist/"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
