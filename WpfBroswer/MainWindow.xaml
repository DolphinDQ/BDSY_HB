<Window x:Class="WpfBroswer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfBroswer"
        mc:Ignorable="d"
        xmlns:form="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <StackPanel Width="200">
            <Button Name="Load" Click="Load_Click">加载地图</Button>
            <Button  Click="ShowVersion_Click">显示版本</Button>
            <DockPanel>
                <Button Click="ChangeVersion_Click">切换版本</Button>
                <TextBox Name="Version"></TextBox>
            </DockPanel>
        </StackPanel>
        <WebBrowser Name="Map" />
    </DockPanel>
</Window>
